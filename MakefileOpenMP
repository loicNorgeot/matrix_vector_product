CXX=icpc
FLAGS=-fopenmp -O3 -lm

vector.o : vector.cpp vector.h
	$(CXX) -c vector.cpp $(FLAGS)

csrmatrix.o : csrmatrix.cpp csrmatrix.h
	$(CXX) -c csrmatrix.cpp $(FLAGS)

mainOpenMP.o : mainOpenMP.cpp vector.h csrmatrix.h
	$(CXX) -c mainOpenMP.cpp $(FLAGS)

main : vector.o csrmatrix.o mainOpenMP.o
	$(CXX) -o main vector.o csrmatrix.o mainOpenMP.o $(FLAGS)
